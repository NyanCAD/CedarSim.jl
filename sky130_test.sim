SkyWater SKY130 transistor test

// Load necessary device models
load "spice/bsim4v8.osdi"

// Simple BSIM4 model card based on SkyWater nfet_01v8 simplified parameters
model sky130_fd_pr__nfet_01v8 sp_bsim4v8 ( type=1
  tnom=27
  toxe=4.148e-9
  vth0=0.52
  k1=0.54
  u0=0.032
  vsat=8e4
  lint=1.2e-8
  wint=2.2e-8
)

// Voltage sources
model v vsource
v1 (1 0) v dc=1.0

// Ground node
subckt test()
  M1 (1 1 0 0) sky130_fd_pr__nfet_01v8 w=1e-6 l=0.5e-6
ends

// Control block
control
  elaborate circuit("test")
  print instance("M1")
  save default
  analysis op1 op
endc
